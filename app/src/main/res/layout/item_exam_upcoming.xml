<?xml version="1.0" encoding="utf-8"?>
<layout>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:background="@drawable/bg_edittext_roundshape"
    android:orientation="vertical">

    <include
        android:id="@+id/incLayoutCourse"
        layout="@layout/item_personal_row_data" />

    <include
        android:id="@+id/incLayoutSem"
        layout="@layout/item_personal_row_data" />

    <include
        android:id="@+id/incLayoutDate"
        layout="@layout/item_personal_row_data" />

    <include
        android:id="@+id/incLayoutBackLog"
        layout="@layout/item_personal_row_data" />

    <include
        android:id="@+id/incLayoutNotice"
        layout="@layout/item_personal_row_data" />

    <include
        android:id="@+id/incLayoutYear"
        layout="@layout/item_personal_row_data" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/btn_timeTable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_button_roundshape"
            android:text="Time Table"
            style="@style/SimpleTextView"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_margin="8dp"
            />

        <Button
            android:id="@+id/btn_hallTicket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_button_roundshape"
            android:text="Hall Ticket"
            style="@style/SimpleTextView"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_margin="8dp"
            />
    </LinearLayout>

</LinearLayout>
</layout>